{
  "manifest_version": 2,
  "name": "AIDUO",
  "version": "1.0",
  "description": "斗图插件",
  "background": {
    "scripts": [
      "background/temp.js"
    ],
    "persistent": true
  },
  "permissions": [
    "*://*/",
    "storage",
    "tabs",
    "clipboardWrite",
    "clipboardRead"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}